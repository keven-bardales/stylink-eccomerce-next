{
  "name": "nextjs-app-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pretty": "prettier --ignore-path .gitignore --cache --write **/*.{ts,tsx,css}",
    "commit": "git-cz",
    "husky:i": "husky install",
    "update": "pnpm up --interactive",
    "update:force": "pnpm up --latest",
    "postinstall": "husky install .husky",
    "version:bump": "cliff-jumper",
    "version:check": "cliff-jumper --dry-run"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@commitlint/format": "^17.4.4",
    "@favware/cliff-jumper": "^2.1.1",
    "@types/node": "20.5.4",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "eslint-plugin-perfectionist": "^1.5.1",
    "eslint-plugin-prettier": "^5.0.0",
    "git-cliff": "^1.2.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "postcss": "8.4.28",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "pretty-quick": "^3.1.3",
    "typescript": "5.1.6"
  }
}